.intro
	position relative
	border-top 1px solid white
	width 100%
	min-height 506px
	background #FFF url('../../img/pic/bg-intro.jpg') no-repeat center -60px
	
.intro::after
	position absolute
	bottom 0px
	display block
	content ''
	width 100%
	height 62px
	background url('../../img/intro-mask.png') no-repeat 50% 0
	background-size 100% 100%
